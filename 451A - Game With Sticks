#include <iostream>
using namespace std;

int main()
{
    int n, m;
    cin >> n >> m;
    
    if(n == 1 || m == 1)
    {
        cout << "Akshat";
        return 0;
    }
    
    int mini = min(n,m);
    
    if(mini%2)
        cout << "Akshat";
    else
        cout << "Malvika";

    return 0;
}
